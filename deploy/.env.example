# =========================================
# Capable.ai — Environment Configuration
# =========================================
# Copy to .env and fill in your values:
#   cp .env.example .env
# =========================================

# Required: Your project credentials (from capable.ai dashboard)
PROJECT_ID=
PROJECT_TOKEN=

# Dashboard auth password (auto-generated if left empty in install.sh)
AUTH_PASSWORD=

# Optional: Subdomain for HTTPS (e.g., "jarvis" → jarvis.capable.ai)
# Leave empty for direct IP:3100 access
SUBDOMAIN=

# Optional: Pack version (defaults to latest)
PACK_VERSION=1

# Optional: OpenClaw version
OPENCLAW_VERSION=2026.2.6-3

# Optional: Gateway token (auto-generated if left empty)
GATEWAY_TOKEN=

# Optional: App URL override
NEXT_PUBLIC_APP_URL=https://capable.ai
