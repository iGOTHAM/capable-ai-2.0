{
  "name": "@capable-ai/prisma",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate": "dotenv -e ../.env -- prisma generate",
    "migrate:dev": "dotenv -e ../.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ../.env -- prisma migrate deploy",
    "db:push": "dotenv -e ../.env -- prisma db push",
    "validate": "dotenv -e ../.env -- prisma validate",
    "studio": "dotenv -e ../.env -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0"
  },
  "devDependencies": {
    "prisma": "^6.3.0",
    "dotenv-cli": "^8.0.0"
  }
}
